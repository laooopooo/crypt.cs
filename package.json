  "description": "Simple string encoder (Base64, CRC32, DES, MD, SHA...).",
  "homepage": "https://bitbucket.org/cedx/crypt.cs",
  "license": "MIT",
  "name": "crypt-cs",
  "private": true,
  "version": "0.4.2",
  "devDependencies": {
    "del": "^1.1.1",
    "docgen-js": "^0.0.1",
    "gulp": "^3.8.11",
    "gulp-david": "^0.2.1",
    "gulp-jshint": "^1.10.0",
    "gulp-load-plugins": "^0.9.0",
    "gulp-zip": "^3.0.2"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "jshintConfig": {
    "camelcase": true,
    "freeze": true,
    "indent": 2,
    "latedef": "nofunc",
    "loopfunc": true,
    "newcap": true,
    "node": true,
    "nonew": true,
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true
  },
  "yuidoc": {
    "logo": "http://dev.belin.io/crypt.cs/favicon.ico",
    "name": "Crypt.cs",
    "url": "https://bitbucket.org/cedx/crypt.cs",
    "options": {
      "extension": ".cs",
      "outdir": "doc/api",
      "paths": "src",
      "tabtospace": 2,
      "theme": "cosmo"
    }
  }
}
